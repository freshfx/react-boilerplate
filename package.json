{
  "name": "react-boilerplate",
  "version": "1.4.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/freshfx/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=10"
  },
  "author": "FreshFX",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "gitcheckremote": "node ./internals/scripts/git-check-remote.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll && rimraf .eslintcache",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "release": "release-it",
    "start": "cross-env NODE_ENV=development node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "jest --config jest.stylelint.config.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --cache",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test TZ=UTC jest --coverage --verbose --config jest.config.json --maxWorkers=50%",
    "test:local": "npm run pretest && cross-env NODE_ENV=test TZ=UTC jest --coverage --verbose --config jest.config.json",
    "test:watch": "cross-env NODE_ENV=test TZ=UTC jest --watchAll --config jest.config.json",
    "prettify": "prettier --write",
    "changelog": "npx conventional-changelog -u | tail -n +3"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@reduxjs/toolkit": "^1.4.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.8.0",
    "core-js": "^3.7.0",
    "cross-env": "7.0.2",
    "deep-freeze": "0.0.1",
    "express": "^4.17.1",
    "fontfaceobserver": "^2.1.0",
    "history": "4.10.1",
    "intl": "1.2.5",
    "ip": "1.1.5",
    "jsonapi-normalizer": "^2.0.1",
    "lodash": "^4.17.20",
    "minimist": "1.2.5",
    "polished": "^4.0.3",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "raf": "^3.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-intl": "^5.10.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-injectors": "^1.3.0",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "sanitize.css": "^12.0.1",
    "serialize-error": "^7.0.1",
    "uuid": "^8.3.1",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@formatjs/intl-pluralrules": "^3.5.5",
    "@formatjs/intl-relativetimeformat": "^7.3.5",
    "@release-it/conventional-changelog": "^2.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "ajv": "^6.12.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-react-intl": "^8.2.16",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "5.2.2",
    "conventional-changelog-cli": "^2.1.1",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-redux-saga": "^1.2.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "1.1.1",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "happypack": "^5.0.1",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "^4.3.0",
    "image-webpack-loader": "^7.0.1",
    "imports-loader": "1.2.0",
    "intl-pluralrules": "^1.2.2",
    "jest-cli": "^26.6.3",
    "jest-emotion": "^10.0.32",
    "jest-runner-stylelint": "^2.3.7",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "redux-saga-test-plan": "^4.0.1",
    "release-it": "^14.2.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-glamorous": "^0.3.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-url-loader": "6.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "moduleRoots": [
    "app"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
